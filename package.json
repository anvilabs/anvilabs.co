{
  "name": "anvilabs.co",
  "version": "0.0.0",
  "private": true,
  "description": "Website for Anvilabs",
  "repository": "anvilabs/anvilabs.co",
  "homepage": "https://github.com/anvilabs/anvilabs.co",
  "license": "GPL-3.0",
  "author": {
    "name": "Ayan Yenbekbay",
    "email": "ayan.yenb@gmail.com",
    "url": "http://yenbekbay.me"
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "scripts": {
    "deploy": "npm run build && netlify deploy",
    "serve": "gatsby serve-build",
    "new:post": "babel-node scripts/new-post",
    "start": "gatsby develop",
    "build": "rimraf public && gatsby build && gulp",
    "test": "npm-run-all --parallel typecheck lint:* jest",
    "test:ci": "npm-run-all --parallel typecheck lint:* jest:coverage && codecov",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "lint:css": "stylelint styles/**/*.scss",
    "lint:css:fix": "stylefmt R styles/",
    "typecheck": "flow check .",
    "commitmsg": "validate-commit-msg",
    "prepush": "npm test"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testPathDirs": [
      "<rootDir>/components"
    ],
    "testRegex": "/__tests__/.*-test\\.js$",
    "collectCoverageFrom": [
      "components/**/*.js"
    ]
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "ci",
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "blog"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.15.3",
    "cheerio": "^0.22.0",
    "classnames": "2.2.5",
    "excerpt-html": "^1.2.0",
    "front-matter": "^2.1.2",
    "highlight.js": "^9.9.0",
    "inline-style-prefixer": "^2.0.5",
    "inquirer": "^3.0.1",
    "js-yaml": "^3.8.1",
    "lodash": "^4.17.4",
    "markdown-it": "^8.2.2",
    "markdown-it-anchor": "^3.0.0",
    "markdown-it-attrs": "^0.8.0",
    "markdown-it-footnote": "^3.0.1",
    "markdown-it-link-attributes": "^1.0.0",
    "markdown-it-table-of-contents": "^0.3.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.17.1",
    "open": "^0.0.5",
    "plural-ru": "^1.1.1",
    "raf": "^3.3.0",
    "react": "^15.4.2",
    "react-disqus-thread": "^0.4.0",
    "react-formal": "^0.24.3",
    "react-headroom": "^2.1.3",
    "react-helmet": "^4.0.0",
    "react-router": "^3.0.2",
    "reading-time": "^1.1.0",
    "rss": "^1.2.2",
    "sitemap": "^1.9.0",
    "slug": "^0.9.1",
    "smoothscroll": "^0.3.0",
    "striptags": "^3.0.1",
    "tachyons-sass": "^4.7.1",
    "toml": "^2.3.1",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-jest": "^18.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "chokidar": "^1.6.1",
    "codecov": "^1.0.1",
    "eslint": "~3.9.1",
    "eslint-config-anvilabs": "^2.1.0",
    "flow-bin": "^0.37.0",
    "gatsby": "^0.12.34",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "gulp-google-webfonts": "^0.0.14",
    "gulp-imagemin": "^3.1.0",
    "gulp-inline-source": "^3.0.0",
    "gulp-minify-inline": "^0.2.1",
    "gulp-uncss": "^1.0.6",
    "husky": "^0.13.1",
    "imagemin-gifsicle": "^5.1.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-optipng": "^5.2.1",
    "imagemin-svgo": "^5.2.0",
    "jest": "^18.1.0",
    "netlify-cli": "^1.1.0",
    "npm-run-all": "^4.0.1",
    "pageres": "^4.3.0",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.4",
    "stylefmt": "^5.1.2",
    "stylelint": "^7.8.0",
    "stylelint-config-primer": "^1.4.0",
    "validate-commit-msg": "^2.11.1"
  }
}
